# ===== 应用配置 =====
APP_NAME=E2E Test Generator
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-here-change-in-production

# ===== 数据库配置 =====
DATABASE_URL=postgresql://postgres:password@localhost:5432/e2e_testgen
REDIS_URL=redis://localhost:6379/0

# ===== Milvus向量数据库配置 =====
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=doc_vectors

# ===== LLM配置 (兼容OpenAI接口) =====
# LLM服务提供商: openai / deepseek / qwen / azure / custom
LLM_PROVIDER=openai

# OpenAI兼容接口配置
LLM_API_KEY=sk-xxx
LLM_BASE_URL=https://api.openai.com/v1
LLM_MODEL=gpt-4o

# Embedding配置 (兼容OpenAI接口)
EMBEDDING_API_KEY=sk-xxx
EMBEDDING_BASE_URL=https://api.openai.com/v1
EMBEDDING_MODEL=text-embedding-3-small

# DeepSeek配置示例 (取消注释使用)
# LLM_PROVIDER=deepseek
# LLM_API_KEY=sk-xxx
# LLM_BASE_URL=https://api.deepseek.com/v1
# LLM_MODEL=deepseek-chat

# 通义千问配置示例 (取消注释使用)
# LLM_PROVIDER=qwen
# LLM_API_KEY=sk-xxx
# LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# LLM_MODEL=qwen-plus

# Azure OpenAI配置示例 (取消注释使用)
# LLM_PROVIDER=azure
# LLM_API_KEY=xxx
# LLM_BASE_URL=https://xxx.openai.azure.com/
# LLM_MODEL=gpt-4
# LLM_API_VERSION=2024-02-15-preview

# ===== 文件存储配置 =====
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=52428800

# ===== Celery配置 =====
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# ===== 日志配置 =====
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log
